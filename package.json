{
  "name": "@y/redis",
  "version": "1.5.3",
  "description": "Scalable websockets provider for Yjs using redis",
  "sideEffects": false,
  "type": "module",
  "funding": {
    "type": "GitHub Sponsors ‚ù§",
    "url": "https://github.com/sponsors/dmonad"
  },
  "scripts": {
    "test": "node --env-file .env tests/index.js",
    "test-inspect": "node --env-file .env --inspect-brk tests/index.js"
  },
  "bin": {
    "y-redis-server": "./bin/server.js",
    "y-redis-worker": "./bin/worker.js"
  },
  "files": [
    "bin/*",
    "src/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "default": "./src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjs/y-redis.git"
  },
  "keywords": [
    "Yjs"
  ],
  "author": "Kevin Jahns <kevin.jahns@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yjs/y-redis/issues"
  },
  "homepage": "https://github.com/yjs/y-redis#readme",
  "standard": {
    "ignore": [
      "/dist",
      "/node_modules"
    ]
  },
  "dependencies": {
    "lib0": "^0.2.93",
    "redis": "^4.6.12",
    "uws": "github:uNetworking/uWebSockets.js#v20.40.0",
    "yjs": "^13.5.6"
  },
  "optionalDependencies": {
    "postgres": "^3.4.3",
    "minio": "^7.1.3"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "rollup": "^4.12.0",
    "@types/node": "^20.11.5",
    "@types/ws": "^8.5.10",
    "concurrently": "^8.2.2",
    "standard": "^17.1.0",
    "typescript": "^5.3.3",
    "ws": "^8.16.0",
    "y-websocket": "^2.0.3"
  }
}
